<Application x:Class="MCRLogManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MCRLogManager"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="GSGMonospace-Regular">
                /MCRLogManager;component/Font/GenShinGothic-Monospace-Regular.ttf#源真ゴシック等幅 Regular
            </FontFamily>
            <FontFamily x:Key="GSGMonospace-Medium">
                /MCRLogManager;component/Font/GenShinGothic-Monospace-Medium.ttf#源真ゴシック等幅 Medium
            </FontFamily>
            <FontFamily x:Key="SHCodeJp-Regular">
                /MCRLogManager;component/Font/SourceHanCodeJP-Regular.otf#源ノ角ゴシック Code JP R
            </FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
